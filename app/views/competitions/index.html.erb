<h2>All Competitions 🏆</h2>

<div class="button-box">
  <%= button_to 'Create a Competition', new_competition_path, :method => :get, :class=>"button"  %>
</div>

<div class="competition-box-index">
  <% @competitions.each do |competition| %>
      <p><%= link_to competition.name, competition %></p>
  <% end %>
</div>
  

